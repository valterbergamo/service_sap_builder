    xmlns:l="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <Page
        showHeader="true"
        title="{i18n>homeTitle}"
    >
        <l:BlockLayout
            background="Default"
        >
            <l:BlockLayoutRow>
                <l:BlockLayoutCell
                    backgroundColorSet="ColorSet9"
